{
    "name": "tritum/repeatable-form-elements",
    "description": "",
    "type": "typo3-cms-extension",
    "require": {
        "typo3/cms-core": "11.5.13",
        "typo3/cms-extbase": "^11.5.13",
        "typo3/cms-form": "^11.5.13"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "repeatable_form_elements"
        }
    },
    "replace": {
        "tritum/repeatable_form_elements": "self.version"
    },
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Ralf Zimemrmann TRITUM GmbH",
            "email": "ralf.zimmermann@tritum.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "TRITUM\\RepeatableFormElements\\": "Classes/"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    }
}
